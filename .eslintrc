{
  "env": {
    "node":true
  },
  "rules": {
    "brace-style": [2],
    "comma-dangle": [2, "never"],
    "comma-spacing": 2,
    "curly": [2, "all"],
    "dot-notation": [2, {"allowKeywords": false}],
    "eol-last": 2,
    "eqeqeq": 2,
    "indent": [2, 2, {"VariableDeclarator": { "var": 2, "let": 2, "const": 3}, }],
    "key-spacing": [1, {"beforeColon": false, "afterColon": true, "mode": "minimum"}],
    "keyword-spacing": 2,
    "linebreak-style": [2, "unix"],
    "new-cap": 0,
    "new-parens": 2,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-console": 0,
    "no-delete-var": 0,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-parens": [2, "functions"],
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-mixed-spaces-and-tabs": [2, true],
    "no-multi-spaces": 0,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-process-exit": 0,
    "no-proto": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-space-before-semi": 0,
    "no-spaced-func": 2,
    "no-trailing-spaces": [2, { "skipBlankLines": false }],
    "no-undef-init": 2,
    "no-underscore-dangle": 1,
    "no-unused-expressions": 2,
    "no-use-before-define": [2, { "functions": false }],
    "no-with": 2,
    "operator-linebreak": [2, "after"],
    "quotes": [2, "double", "avoid-escape"],
    "semi": 2,
    "semi-spacing": [2, {"before": false, "after": true}],
    "space-before-function-paren": [2, "never"],
    "space-infix-ops": 2,
    "space-unary-ops": 1,
    "strict": 1,
    "yoda": [2, "never"],
    "camelcase": 1
  }
}
